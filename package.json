{
  "name": "cquant",
  "version": "0.1.4",
  "description": "A fast and native image palette generator",
  "main": "cquant.js",
  "types": "./cquant.d.ts",
  "scripts": {
    "install": "node-gyp-build",
    "test": "npm run predev && node ./test/test.js && node ./test/api-test.js && node ./test/perf.js",
    "tsc": "tsc --build",
    "prebuild": "prebuildify --napi",
    "predev": "npm install image-palette image-pixels which"
  },
  "keywords": [
    "image-palette",
    "color-quant",
    "cquant",
    "palette",
    "quant",
    "MMCQ"
  ],
  "author": "xVanTuring",
  "repository": {
    "type": "git",
    "url": "git://github.com/xVanTuring/cquant"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.2",
    "node-addon-api": "^1.6.3",
    "node-gyp-build": "^4.1.0",
    "prebuildify": "^3.0.0",
    "sharp": "^0.22.1"
  },
  "devDependencies": {
    "@types/node": "^10.14.4",
    "typescript": "^3.4.3",
    "which": "^1.3.1"
  },
  "binary": {
    "napi_versions": [
      3,
      4,
      4
    ]
  }
}