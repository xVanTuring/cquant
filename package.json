{
  "name": "cquant",
  "version": "0.1.4",
  "description": "A fast and native image palette generator",
  "main": "cquant.js",
  "types": "./cquant.d.ts",
  "scripts": {
    "install": "prebuild-install || (npm run configure && node-gyp build)",
    "configure": "node-gyp configure",
    "test": "npm run predev && node ./test/test.js && node ./test/api-test.js && node ./test/perf.js && node ./script/build-deploy.js",
    "tsc": "tsc --build",
    "predev": "npm install image-palette image-pixels sharp prebuild"
  },
  "keywords": [
    "image-palette",
    "color-quant",
    "cquant",
    "palette",
    "quant",
    "quantization",
    "MMCQ"
  ],
  "author": "xVanTuring",
  "repository": {
    "type": "git",
    "url": "git://github.com/xVanTuring/cquant"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.2",
    "bindings": "^1.5.0",
    "node-addon-api": "^1.6.3",
    "prebuild-install": "^5.2.5"
  },
  "devDependencies": {
    "@types/node": "^10.14.4",
    "typescript": "^3.4.3",
    "which": "^1.3.1"
  }
}
